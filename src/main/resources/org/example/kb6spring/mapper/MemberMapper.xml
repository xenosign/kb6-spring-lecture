<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.kb6spring.mapper.MemberMapper">
    <select id="findAll" resultType="MemberEntity">
        select *
        from member;
    </select>
    <select id="findOne" resultType="MemberEntity">
        select *
        from member
        LIMIT 2;
    </select>
</mapper>